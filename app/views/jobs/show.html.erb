<%- model_class = Jobs -%>
<div class="page-header">
  <h1><%=t '.title', :default => Jobs %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:started_at) %></dt>
      <dd><pre class="prettyprint"><%= @jobs.started_at %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:finished_at) %></dt>
      <dd><pre class="prettyprint"><%= @jobs.finished_at %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              jobs_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_jobs_path(@jobs), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              jobs_path(@jobs),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

